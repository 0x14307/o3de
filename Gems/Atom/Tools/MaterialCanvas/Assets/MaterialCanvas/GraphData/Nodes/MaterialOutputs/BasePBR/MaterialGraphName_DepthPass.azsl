VsOutput MainVS(VsInput IN)
{
    return EvaluateVertexGeometry(IN);
}

struct PsOutput
{
    precise float m_depth : SV_Depth;
};

PsOutput MainPS(VsOutput IN, bool isFrontFace : SV_IsFrontFace)
{
    PsOutput OUT;
    OUT.m_depth = IN.position.z;
    return OUT;
}